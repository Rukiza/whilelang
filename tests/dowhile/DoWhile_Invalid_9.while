int f(int[] items) {
  int i = 0;
  int r;
  do {
      do {
        i = i + 1;
        if (1 == 4) {
            r = i;
            break;
        }
        else {
            continue;
        }
        r = i;
      } while (i < |items|);
  } while(i < |items|);

  return r;
}

void main() {
    assert f([1]) == 1;
    assert f([1,2,3]) == 3;
    assert f([-1]) == 1;
}
